{
  "cluster": {
    "name": "oaswarm",
    "external-network-id": "public"
  },
  "counts": {
    "master-count": 1,
    "node-count": 1
  },
  "osGlobalOptions": {
    "os-auth-url": "@import!secrets.json#os-auth-url",
    "os-project-domain-name": "default",
    "os-user-domain-name": "default",
    "os-project-name": "@import!secrets.json#os-project-name",
    "os-username": "@import!secrets.json#os-username",
    "os-password": "@import!secrets.json#os-password",
    "os-identity-api-version" :"@import!secrets.json#os-identity-api-version"
  }
}
